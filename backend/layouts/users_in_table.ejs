<!DOCTYPE html>
<html>
<head>
    <title>Fake Users Table</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 20px;
        }
        th, td {
            border: 1px solid #ccc;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <h1>User Details Table</h1>
    <table>
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Age</th>
                <th>Account Number</th>
                <th>Bank Name</th>
                <th>Account Type</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.firstName %></td>
                    <td><%= user.lastName %></td>
                    <td><%= user.email %></td>
                    <td><%= user.age %></td>
                    <td><%= user.financialDetails.accountNumber %></td>
                    <td><%= user.financialDetails.bankName %></td>
                    <td><%= user.financialDetails.accountType %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
